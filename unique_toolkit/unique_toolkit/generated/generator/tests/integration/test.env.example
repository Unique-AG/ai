# Example environment file for integration tests
# Copy to test.env and fill in your values

# Unique API Settings
UNIQUE_APP_KEY=your_app_key_here
UNIQUE_APP_ID=your_app_id_here
UNIQUE_COMPANY_ID=your_company_id_here
UNIQUE_USER_ID=your_user_id_here
UNIQUE_API_VERSION=2024-10-01

# Base URL
UNIQUE_BASE_URL=https://gateway.qa.unique.app/public/chat-gen2

# Test Data IDs (existing resources for read-only tests)
TEST_CHAT_ID=your_test_chat_id
TEST_FOLDER_SCOPE_ID=your_test_folder_id
TEST_ASSISTANT_ID=your_test_assistant_id
TEST_CONTENT_ID=your_test_content_id
TEST_MESSAGE_ID=your_test_message_id
TEST_TABLE_ID=your_test_table_id

# Note: CRUD tests will create and delete their own resources
# Read-only tests use these existing resource IDs

